StudentsWithMarks(StudentId, CourseId) :-
    Marks(StudentId, CourseId, _).

SEL(GroupId, CourseId) :-
    Students(StudentId, _, GroupId),
    Courses(CourseId, _),
    not StudentsWithMarks(StudentId, CourseId).

RES(GroupId, CourseId) :-
    Groups(GroupId, _),
    Courses(CourseId, _),
    not SEL(GroupId, CourseId).
